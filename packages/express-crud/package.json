  "devDependencies": {
    "@types/express": "^4.17.0",
    "@varasto/mock-storage": "^1.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.5"
  }
