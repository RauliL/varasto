{
  "name": "@varasto/express-crud",
  "version": "1.0.0",
  "description": "Express.js CRUD generator that uses Varasto storage",
  "keywords": [
    "varasto",
    "server",
    "express",
    "crud"
  ],
  "author": "Rauli Laine <rauli.laine@iki.fi> (https://rauli.dev/)",
  "license": "MIT",
  "homepage": "https://github.com/RauliL/varasto",
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/varasto"
  },
  "bugs": {
    "url": "https://github.com/RauliL/varasto/issues"
  },
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint './src/*.ts'",
    "format": "prettier --write './src'",
    "format:check": "prettier --check './src'",
    "test": "jest",
    "prepack": "yarn run build"
  },
  "main": "./dist/index.js",
