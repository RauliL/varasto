{
  "name": "@varasto/express-crud",
  "version": "2.0.0",
  "description": "Express.js CRUD generator that uses Varasto storage",
  "keywords": [
    "varasto",
    "server",
    "express",
    "crud"
  ],
  "author": "Rauli Laine <rauli.laine@iki.fi>",
  "license": "MIT",
  "main": "./dist/index.js",
  "main:src": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=12.0"
  },
  "homepage": "https://github.com/RauliL/varasto",
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/varasto"
  },
  "bugs": {
    "url": "https://github.com/RauliL/varasto/issues"
  },
  "eslintConfig": {
    "extends": "../../eslintrc.js"
  },
  "scripts": {
    "clean": "rimraf './dist'",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint './src/*.ts'",
    "format": "prettier --write './src'",
    "format:check": "prettier --check './src'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "@varasto/storage": "^3.0.0",
    "express": "^4.17.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@varasto/memory-storage": "^1.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "yarn": "^1.22.10"
  }
}
