{
  "name": "@varasto/server",
  "version": "2.0.0",
  "description": "JSON key-value store with HTTP server interface",
  "keywords": [
    "varasto",
    "server"
  ],
  "author": "Rauli Laine <rauli.laine@iki.fi> (https://rauli.dev/)",
  "license": "MIT",
  "homepage": "https://github.com/RauliL/varasto",
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/varasto"
  },
  "bugs": {
    "url": "https://github.com/RauliL/varasto/issues"
  },
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "start": "./bin/varasto",
    "start:dev": "nodemon --watch './src/*.ts' --exec './bin/varasto'",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf './dist'",
    "lint": "eslint './src/*.ts'",
    "test": "jest",
    "prepack": "yarn run build"
  },
  "bin": {
    "varasto": "./bin/varasto"
  },
  "main": "./dist/index.js",
  "main:src": "./src/index.ts",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/express": "^4.17.0",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.8",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@varasto/mock-storage": "^1.1.0",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@varasto/storage": "^2.0.0",
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "yargs": "^16.2.0"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc.js"
  }
}
