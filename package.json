{
  "name": "varasto",
  "version": "0.1.0",
  "description": "JSON storage with HTTP server interface",
  "author": "Rauli Laine <rauli.laine@iki.fi> (https://rauli.dev)",
  "license": "MIT",
  "homepage": "https://github.com/RauliL/varasto",
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/varasto"
  },
  "bugs": {
    "url": "https://github.com/RauliL/varasto/issues"
  },
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "start": "nodemon --watch 'src/*.ts' --exec 'ts-node ./src/index.ts'",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "prepack": "npm run build"
  },
  "bin": {
    "varasto": "./bin/varasto"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/mkdirp": "^0.5.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.8",
    "@types/yargs": "^13.0.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
}
